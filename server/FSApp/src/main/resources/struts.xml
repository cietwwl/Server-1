<struts>  
	<constant name="struts.action.11excludePattern" value="/download"/>
	<constant name="struts.allowed.action.names" value="[a-zA-Z]*" />
    <package name="" namespace="/" extends="struts-default">  
       
        <action name="service" class="fsHttpControler" method="doService">  
              <result type="plainText">
